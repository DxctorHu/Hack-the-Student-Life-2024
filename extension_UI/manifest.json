{
    "manifest_version": 3,
    "name": "Sticky Note",
    "version": "1.0",
    "description": "Hack student life sticky note",
    "permissions": ["contextMenus", "activeTab", "scripting"],
    "background": {
      "service_worker": "scripts\/background.js"
    },
    "action": {
      "default_popup": "scripts\/sidebar.html",
      "default_icon": "icons\/hello_extensions.png"
    },
    "host_permissions": [
      "<all_urls>"
    ]
}
